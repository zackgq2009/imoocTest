<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  
  <servlet>
    <servlet-name>SignUp</servlet-name>
    <servlet-class>com.tsingkuo.webapp.servlet.SignUp</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>SignIn</servlet-name>
    <servlet-class>com.tsingkuo.webapp.servlet.SignIn</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>CartServlet</servlet-name>
    <servlet-class>com.tsingkuo.webapp.servlet.CartServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>SignOut</servlet-name>
    <servlet-class>com.tsingkuo.webapp.servlet.SignOut</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>SignUp</servlet-name>
    <url-pattern>/servlet/SignUp</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>SignIn</servlet-name>
    <url-pattern>/servlet/SignIn</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>CartServlet</servlet-name>
    <url-pattern>/servlet/CartServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>SignOut</servlet-name>
    <url-pattern>/servlet/SignOut</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>viewFiles/welcome.jsp</welcome-file>
  </welcome-file-list>

</web-app>
