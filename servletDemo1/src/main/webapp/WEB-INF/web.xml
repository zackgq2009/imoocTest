<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>MyServletDemo1</servlet-name>
    <servlet-class>servlet.MyServletDemo1</servlet-class>
  </servlet>

  <servlet>
  <servlet-name>MyServletFile1</servlet-name>
  <servlet-class>servlet.MyServletFile1</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>MyServletDemo1</servlet-name>
    <url-pattern>/servlet/myservletdemo1</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>MyServletFile1</servlet-name>
    <url-pattern>/servlet/myservletfile1</url-pattern>
  </servlet-mapping>

</web-app>
